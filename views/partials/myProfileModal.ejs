<!-- Modal -->
<div id="myProfileModal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Profile</h4>
            </div>
            <div class="modal-body row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                      <ul class="list-group stat-panel">
                          <li class="list-group-item row">
                              <div class="col-md-6">Username:</div>
                              <div class="col-md-6 profile-username"></div>
                          </li>
                          <li class="list-group-item row">
                              <div class="col-md-6">Account Type:</div>
                              <div class="col-md-6 profile-type"></div>
                          </li>
                          <li class="list-group-item row">
                              <div class="col-md-6">Country: </div>
                              <div class="col-md-6 profile-country"></div>
                          </li>
                          <li class="list-group-item row">
                              <div class="col-md-6">Rating: </div>
                              <div class="col-md-6 profile-rating"></div>
                          </li>
                      </ul>
                        <form method="post" id="uploadListing">
                            <div class="form-group ">
                                <label class="control-label " for="item">Username<span class="asteriskField">*</span></label>
                                <input class="form-control" id="item" name="item" type="text" minlength="2" required/>
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="image">Country</label>
                                <input class="form-group" id="image" name="image" type="file" />
                                <img id="imagePreview" src="#" style="max-width:50%" ;/>
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="cs1">Rating</label>
                                <input class="control-label" id="cs1" name="country" type="text" />
                            </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
            </form>
        </div>
    </div>
</div>

<script src="/js/profiles.js"></script>
